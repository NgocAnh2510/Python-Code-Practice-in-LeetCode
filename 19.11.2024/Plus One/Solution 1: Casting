class Solution:
    def plusOne(self, digits: List[int]) -> List[int]:
        st = ""
        for i in digits:
            st += str(i)
        str_num = int(st)
        str_num_plus = str_num + 1
        str_num_plus_str = str(str_num_plus)
        list = [int(x) for x in str_num_plus_str]
        return list

# Runtime: 0 ms (beats 100.00%)
# Memory: 16.61 MB (beats 24.69%)
